# Generate the dependencies to the various raw *.txt files.
BASE_TARGETS := nano.txt micro.txt samd.txt stm32.txt esp8266.txt esp32.txt \
	teensy32.txt
TARGETS := $(foreach target,$(BASE_TARGETS),MemoryBenchmark/${target})
TARGETS += $(foreach target,$(BASE_TARGETS),CpuBenchmark/${target})

# Force 'make' without target do nothing, so that the README.md does not
# become autogenerated during automated testing through ../Makefile.
all:

# Do nothing for clean
clean:

README.md: generate_readme.py generate_combined.awk $(TARGETS)
	./generate_readme.py > $@
